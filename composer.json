{
  "name": "codexten/yii-pdf",
  "type": "library",
  "description": "PDF",
  "require": {
    "yii2tech/html2pdf": "*",
    "mpdf/mpdf": "^6.0.0|^7.0.0"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "codexten/yii-dev": "^2.0.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "codexten\\yii\\pdf\\": "src",
      "codexten\\yii\\pdf\\tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    },
    "config-plugin": {
      "params": "config/params.php",
      "pdf": "config/pdf.php",
      "testPdf": [
        "$base",
        "$pdf",
        "tests/config/testPdf.php"
      ]
    }
  }
}
